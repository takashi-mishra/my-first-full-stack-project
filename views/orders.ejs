<!DOCTYPE html>
<html lang="en">
<head>
   <meta charset="UTF-8">
   <meta name="viewport" content="width=device-width, initial-scale=1.0">
   <link rel="stylesheet" href="/stylesheet/orders.css">
   <title>Your Orders</title>
</head>
<body>
   <h1>Your Orders</h1>
   <ul>
       <% orders.forEach(function(order) { %>
           <li>
               <strong>Product Name:</strong> <%= order.productName %><br>
               <strong>Ordered By:</strong> <%= order.userName %><br>
               <strong>Order Date:</strong> <%= order.orderDate.toDateString() %><br>
               <strong>Status:</strong> <%= order.status %><br>
           </li>
       <% }); %>
   </ul>
</body>
</html>
